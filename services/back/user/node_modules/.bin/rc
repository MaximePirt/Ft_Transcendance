#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tdausque/Projects/Ft_Transcendance/services/back/user/node_modules/rc/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/back/user/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/back/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/node_modules:/home/tdausque/Projects/Ft_Transcendance/node_modules:/home/tdausque/Projects/node_modules:/home/tdausque/node_modules:/home/node_modules:/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/back/user/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tdausque/Projects/Ft_Transcendance/services/back/user/node_modules/rc/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/back/user/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/back/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/node_modules:/home/tdausque/Projects/Ft_Transcendance/node_modules:/home/tdausque/Projects/node_modules:/home/tdausque/node_modules:/home/node_modules:/node_modules:/home/tdausque/Projects/Ft_Transcendance/services/back/user/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rc/cli.js" "$@"
else
  exec node  "$basedir/../rc/cli.js" "$@"
fi
